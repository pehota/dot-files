{
  "languageserver": {
    "elmLS": {
      "command": "elm-language-server",
      "args": ["--stdio"],
      "filetypes": ["elm"],
      "rootPatterns": ["elm.json"],
      "initializationOptions": {
        "elmPath": "elm",
        "elmFormatPath": "elm-format",
        "elmTestPath": "elm-test",
        "diagnosticsOnSaveOnly": true
      }
    },
    "efm": {
      "command": "efm-langserver",
      "args": [],
      "filetypes": ["vim", "erb", "eruby", "markdown"]
    },
    "haskell": {
      "command": "haskell-language-server-wrapper",
      "args": ["--lsp"],
      "rootPatterns": [
        "*.cabal",
        "stack.yaml",
        "cabal.project",
        "package.yaml",
        "hie.yaml"
      ],
      "filetypes": ["haskell", "lhaskell"],
      "initializationOptions": {
        "haskell": {}
      }
    }
  },
  "diagnostic.refreshOnInsertMode": false,
  "diagnostic.maxWindowHeight": 20,
  "suggest.maxCompleteItemCount": 5,
  "coc.preferences.formatOnSaveFiletypes": [
    "elm",
    "typescript",
    "typescriptreact",
    "javascript",
    "javascriptreact",
    "javascript.node",
    "json",
    "haskell",
    "css",
    "scss",
    "sass",
    "html",
    "html.handlebars",
    "vue",
    "rust",
    "sh"
  ],
  "typescript.updateImportsOnFileMove.enable": true,
  "codeLens.enable": true,
  "prettier.eslintIntegration": true,
  "prettier.requireConfig": false,
  "eslint.autoFixOnSave": true,
  "eslint.quiet": true,
  "eslint.options": {
    "useEslintrc": true
  },
  "css.validate": false,
  "stylelint.enable": true,
  "stylelint.filetypes": ["css", "less", "scss", "sass", "vue", "html"],
  "sql.database": "postgresql"
}
